    "description"
  ],
  "author": "Peter Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peter-evans/dockerhub-description/issues"
  },
  "homepage": "https://github.com/peter-evans/dockerhub-description#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "node-fetch": "^2.6.9",
    "unicode-substring": "^0.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.18.25",
    "@typescript-eslint/parser": "^5.59.1",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.39.0",
    "eslint-plugin-github": "^4.7.0",
    "eslint-plugin-jest": "^25.7.0",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^27.1.5",
    "typescript": "^4.9.5"
  }
